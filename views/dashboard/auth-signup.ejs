    <link rel="stylesheet" href="dashboard/css/fonts/fontawesome.css">

    <link rel="stylesheet" href="dashboard/css/login-style.css">
    <link rel="stylesheet" href="dashboard/css/customizer.css">
    <link rel="stylesheet" href="dashboard/css/login-custom.css">
    <style>
        #signup-logo {
            margin-top: 1.5rem;

        }
    </style>
</head>


<div class="auth-wrapper" style="background-color:transparent;">
    <div class="auth-content col-md-8">
        <div class="card">
            <div class="row align-items-center text-center">
                <div class="col-md-12 col-sm-11">
                    <style>
                        small {
                            display: block;
                            width: 100% !important;
                        }
                    </style>
                    <div class="card-body p-1">
                        <img src="dashboard/images/payfoxtrade.jpg" id="signup-logo" alt=""
                            class="img-fluid mb-4 col-md-3 col-4 mx-auto">
                        <h4 class="mb-3 f-w-400">Sign Up</h4>
                        <% if(error){ %>
                            <div>
                                <p class="text-danger">
                                    <%= error %>
                                </p>
                            </div>
                            <% } %>

                    </div>
                    <!-- <form> -->

                        <div class="input-group mb-3 col-sm-5 mx-auto col-12 p-0">
                            <div class="input-group-prepend">
                                <span class="input-group-text"><i data-feather="users"></i></span>

                            </div>
                            <input type="text" name="firstname" class="form-control" value="" placeholder="Firstname"
                                minlength="2" maxlength="20">
                            <small class="badge-light-danger text-danger" name="firstname-e"></small>

                        </div>
                        <div class="input-group mb-3 col-sm-5 mx-auto col-12 p-0">
                            <div class="input-group-prepend">
                                <span class="input-group-text"><i data-feather="feather"></i></span>
                            </div>
                            <input type="text" name="lastname" class="form-control" value="" placeholder="Lastname"
                                minlength="2" maxlength="20">
                            <small class="badge-light-danger text-danger" name="lastname-e"></small>

                        </div>
                        <div class="input-group mb-3 col-sm-5 mx-auto col-12 p-0">
                            <div class="input-group-prepend">
                                <span class="input-group-text"><i data-feather="user"></i></span>
                            </div>
                            <input type="text" name="username" class="form-control" value="" placeholder="Username"
                                minlength="2" maxlength="20">
                            <small class="badge-light-danger text-danger" name="username-e"></small>
                        </div>
                        <div class="input-group mb-3 col-sm-5 mx-auto col-12 p-0">
                            <div class="input-group-prepend">
                                <span class="input-group-text"><i data-feather="mail"></i></span>
                            </div>
                            <input type="email" name="email" class="form-control" value="" placeholder="Email address">
                            <small class="badge-light-danger text-danger" name="email-e"></small>

                        </div>

                        <div class="input-group mb-3 col-sm-5 mx-auto col-12 p-0">
                            <div class="input-group-prepend">
                                <span class="input-group-text"><i data-feather="user"></i></span>
                            </div>
                            <input type="text" name="upline" class="form-control" id="upline" value=""
                                placeholder="Referral code">

                            <small class="badge-light-danger text-danger" ></small>

                        </div>
                        <div class="input-group mb-4 col-sm-5 mx-auto col-12 p-0">
                            <div class="input-group-prepend">
                                <span class="input-group-text"><i data-feather="lock"></i></span>
                            </div>
                            <input type="password" name="password" class="form-control" value="" placeholder="Password">
                            <small class="badge-light-danger text-danger" name="password-e"></small>

                        </div>
                        <div class="input-group mb-4 col-sm-5 mx-auto col-12 p-0">
                            <div class="input-group-prepend">
                                <span class="input-group-text"><i data-feather="pocket"></i></span>
                            </div>
                            <input type="password" name="cpassword" class="form-control" value=""
                                placeholder="Confirm Password">
                            <small class="badge-light-danger text-danger" name="cpassword-e"></small>
                        </div>
                        <div class="form-group text-left mt-2 col-12 text-center">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" name="consent" class="custom-control-input input-primary"
                                    id="consent" checked disabled>
                                <label class="custom-control-label" for="consent">I agree to the <a href="#!"> Terms &
                                        Conditions</a>.</label>
                            </div>
                            <small class="badge-light-danger text-danger" name="consent-e"></small>
                        </div>
                        <button id="btnSignup"
                            class="btn btn-primary col-md-5 col-12 mx-auto mb-4" disabled name="signup">Sign up</button>
                    <!-- </form> -->
                    <p class="mb-2">Already have an account? <a href="/login" class="f-w-400">Signin</a></p>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
<script src="dashboard/js/reg-script.js" type="text/javascript"></script>
<script src="dashboard/js/vendor-all.min.js"></script>
<script src="dashboard/js/plugins/bootstrap.min.js"></script>
<script src="dashboard/js/plugins/feather.min.js"></script>
<script src="dashboard/js/pcoded.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.dashboard/js/9.12.0/highlight.min.js"></script>
<script src="dashboard/js/plugins/clipboard.min.js"></script>
<script src="dashboard/js/uikit.min.js"></script>

<script src="assets/dashboard/js/plugins/clipboard.min.html"></script>
<script type="text/javascript">
    var iconlist = ['alert-octagon', 'alert-circle', 'activity', 'alert-triangle', 'align-center', 'airplay', 'align-justify', 'align-left', 'align-right', 'arrow-down-left',
        'arrow-down-right', 'anchor', 'aperture', 'arrow-left', 'arrow-right', 'arrow-down', 'arrow-up-left', 'arrow-up-right', 'arrow-up', 'award', 'bar-chart', 'at-sign',
        'bar-chart-2', 'battery-charging', 'bell-off', 'battery', 'bluetooth', 'bell', 'book', 'briefcase', 'camera-off', 'calendar', 'bookmark', 'box', 'camera',
        'check-circle', 'check', 'check-square', 'cast', 'chevron-down', 'chevron-left', 'chevron-right', 'chevron-up', 'chevrons-down', 'chevrons-right', 'chevrons-up',
        'chevrons-left', 'circle', 'clipboard', 'chrome', 'clock', 'cloud-lightning', 'cloud-drizzle', 'cloud-rain', 'cloud-off', 'codepen', 'cloud-snow', 'compass', 'copy',
        'corner-down-right', 'corner-down-left', 'corner-left-down', 'corner-left-up', 'corner-up-left', 'corner-up-right', 'corner-right-down', 'corner-right-up', 'cpu', 'credit-card',
        'crosshair', 'disc', 'delete', 'download-cloud', 'download', 'droplet', 'edit-2', 'edit', 'external-link', 'eye', 'feather', 'facebook', 'file-minus',
        'eye-off', 'fast-forward', 'file-text', 'film', 'file', 'file-plus', 'folder', 'filter', 'flag', 'globe', 'grid', 'heart', 'home', 'github', 'image',
        'inbox', 'layers', 'info', 'instagram', 'layout', 'link-2', 'life-buoy', 'link', 'log-in', 'list', 'lock', 'log-out', 'loader', 'mail', 'maximize-2',
        'map', 'map-pin', 'menu', 'message-circle', 'message-square', 'minimize-2', 'mic-off', 'minus-circle', 'mic', 'minus-square', 'minus', 'moon', 'monitor',
        'more-vertical', 'more-horizontal', 'move', 'music', 'navigation-2', 'navigation', 'octagon', 'package', 'pause-circle', 'pause', 'percent', 'phone-call',
        'phone-forwarded', 'phone-missed', 'phone-off', 'phone-incoming', 'phone', 'phone-outgoing', 'pie-chart', 'play-circle', 'play', 'plus-square', 'plus-circle', 'plus',
        'pocket', 'printer', 'power', 'radio', 'repeat', 'refresh-ccw', 'rewind', 'rotate-ccw', 'refresh-cw', 'rotate-cw', 'save', 'search', 'server', 'scissors',
        'share-2', 'share', 'shield', 'settings', 'skip-back', 'shuffle', 'sidebar', 'skip-forward', 'slack', 'slash', 'smartphone', 'square', 'speaker', 'star',
        'stop-circle', 'sun', 'sunrise', 'tablet', 'tag', 'sunset', 'target', 'thermometer', 'thumbs-up', 'thumbs-down', 'toggle-left', 'toggle-right', 'trash-2', 'trash',
        'trending-up', 'trending-down', 'triangle', 'type', 'twitter', 'upload', 'umbrella', 'upload-cloud', 'unlock', 'user-check', 'user-minus', 'user-plus', 'user-x',
        'user', 'users', 'video-off', 'video', 'voicemail', 'volume-x', 'volume-2', 'volume-1', 'volume', 'watch', 'wifi', 'x-square', 'wind', 'x', 'x-circle',
        'zap', 'zoom-in', 'zoom-out', 'command', 'cloud', 'hash', 'headphones', 'underline', 'italic', 'bold', 'crop', 'help-circle', 'paperclip', 'shopping-cart',
        'tv', 'wifi-off', 'minimize', 'maximize', 'gitlab', 'sliders'
    ];

    for (var i = 0, l = iconlist.length; i < l; i++) {
        $('#icon-wrapper').append(
            '<div class="i-block" data-clipboard-text="' + iconlist[i] + '" data-filter="' + iconlist[i] + '"  data-toggle="tooltip" title="' + iconlist[i] + '">' +
            '<i data-feather="' + iconlist[i] + '"></i>' +
            '</div>');
    }
    $(window).on('load', function () {
        let ref_code = window.location.href.split("=")[1];
        if (ref_code) {
            document.getElementById("upline").value = ref_code
        }
        var iclp = new ClipboardJS('.i-block');
        iclp.on('success', function (e) {
            $(e.trigger).append("<span class='ic-badge badge badge-success'>copied</span>");
            setTimeout(function () {
                $('.i-block').children('.ic-badge').remove();
            }, 3000);
        });

        iclp.on('error', function (e) {
            $(e.trigger).append("<span class='ic-badge badge badge-danger'>Error</span>");
            setTimeout(function () {
                $('.i-block').children('.ic-badge').remove();
            }, 3000);
        });

        $("#icon-search").on("keyup", function () {
            var g = $(this).val().toLowerCase();
            $(".i-main .i-block").each(function () {
                var t = $(this).attr('data-filter');
                if (t) {
                    var s = t.toLowerCase();
                }
                if (s) {
                    var n = s.indexOf(g);
                    if (n !== -1) {
                        $(this).show();
                    } else {
                        $(this).hide();
                    }
                }
            });
        });
    });
</script>
